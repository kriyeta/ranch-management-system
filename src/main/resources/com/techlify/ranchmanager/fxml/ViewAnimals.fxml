<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:id="viewBox" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.techlify.ranchmanager.controller.ViewAnimalsController">
	<children>
		<VBox fx:id="filterBox" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
			<children>
				<Label text="Enter text bellow to filter:">
					<font>
						<Font name="System Bold" size="16.0" />
					</font>
				</Label>
				<TextField fx:id="filterText" maxWidth="1.7976931348623157E308" VBox.vgrow="SOMETIMES" />
				<TableView fx:id="animalsTable" VBox.vgrow="SOMETIMES">
					<columns>
						<TableColumn fx:id="id" maxWidth="1.7976931348623157E308" minWidth="75.0" prefWidth="-1.0" text="ID" />
						<TableColumn fx:id="numbers" maxWidth="1.7976931348623157E308" minWidth="90.0" prefWidth="-1.0" text="NUMBERS" />
						<TableColumn fx:id="type" maxWidth="1.7976931348623157E308" minWidth="75.0" prefWidth="-1.0" text="TYPE" />
						<TableColumn fx:id="dateOfBirth" maxWidth="1.7976931348623157E308" minWidth="150.0" prefWidth="-1.0" text="DATE OF BIRTH" />
						<TableColumn fx:id="gender" maxWidth="1.7976931348623157E308" minWidth="90.0" prefWidth="-1.0" text="GENDER" />
					</columns>
				</TableView>
			</children>
		</VBox>
	</children>
</AnchorPane>
